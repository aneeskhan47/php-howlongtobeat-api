{
    "name": "aneeskhan47/php-howlongtobeat-api",
    "description": "A PHP API wrapper for HowLongToBeat.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anees Khan",
            "email": "kingkhan2388@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AneesKhan47\\HowLongToBeat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AneesKhan47\\HowLongToBeat\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --display-phpunit-deprecations --display-deprecations"
    }
}